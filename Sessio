class Sessio{
    // BÉ
    private String obra;
    private double preu;
    private String hora;
    private String dia;
    private Espectador seients[][];
    // Objeto[][] matriz = new Objeto[3][3];


    // Constructor 1 \\
    public Sessio(String obra, double preu, String hora, String dia, Espectador seients[][]) {
        this.obra = obra;
        this.preu = preu;
        this.hora = hora;
        this.dia = dia;
        this.seients = seients;
    }

    public String getObra(){ // BÉ
        return this.obra;
    }

    public double getPreu(){ // BÉ
        return this.preu;
    }

    public String getHora(){ // BÉ
        return this.hora;
    }

    public String getDia(){ // BÉ
        return this.dia;
    }

    public Espectador[][] getSeient(){ // BÉ
        return this.seients;
    }

    public void setObra(String obra){ // BÉ
        this.obra = obra;
    }

    public void setPreu(double preu){ // BÉ
        this.preu = preu;
    }

    public void setHora(String hora){ // BÉ
        this.hora = hora;
    }

    public void setDia(String dia){ // BÉ
        this.dia = dia;
    }

    public void setSeients(Espectador seients[][]){ // BÉ
        this.seients = seients;
    }

